    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.6.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "jest": "^28.1.3",
    "prisma": "3.15.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
